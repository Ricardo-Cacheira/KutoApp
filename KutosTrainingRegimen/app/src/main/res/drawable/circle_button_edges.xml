<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true" >
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#35a7a9" />
            <gradient android:angle="90" android:startColor="#216d6e" android:endColor="#32a2a4"  />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#32a2a4" />
            <solid android:color="#2e9294"/>
        </shape>
    </item>
    <item >
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#257779" />
            <gradient android:angle="90" android:startColor="#267b7e" android:endColor="#34a9ac" />
        </shape>
    </item>
</selector>